<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
	<record id="view_partner_structured_form_branch" model="ir.ui.view">
        <field name="name">view_partner_structured_form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='sales_purchases']/group[@name='container_row_2']/group[@name='misc']/field[@name='company_id']"
                   position="after">
                <field name="branch_id"/>
            </xpath>
        </field>
    </record>
</odoo>